<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.jku.Controller">
    <children>
        <Label layoutX="113.0" layoutY="71.0" text="Raum erstellen">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label layoutX="63.0" layoutY="166.0" text="Größe" />
        <TextField fx:id="roomSize" layoutX="110.0" layoutY="163.0" onAction="#getRoomSize" />
        <Button fx:id="bestätigenButton" layoutX="149.0" layoutY="230.0" mnemonicParsing="false" onAction="#createNewRoom" text="Bestätigen" />
        <Button fx:id="backToRoomOverviewButton" layoutX="88.0" layoutY="304.0" mnemonicParsing="false" onAction="#switchToStartScene" text="Zurück zur Room Overview" />
        <TextField fx:id="roomId" layoutX="110.0" layoutY="129.0" onAction="#getRoomId" />
        <Label layoutX="55.0" layoutY="133.0" text="Room ID" />
        <TextField fx:id="measurementUnit" layoutX="110.0" layoutY="194.0" onAction="#getMeasurementUnit" />
        <Label layoutX="66.0" layoutY="198.0" text="Unit" />
        <Label fx:id="createdRoom" layoutX="100.0" layoutY="262.0" text="|" />
    </children>
</AnchorPane>
