<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="538.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.jku.Controller">
    <children>
        <Label layoutX="99.0" layoutY="101.0" text="Raum updaten">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label layoutX="68.0" layoutY="151.0" text="Größe" />
        <TextField fx:id="roomSize" layoutX="107.0" layoutY="147.0" onAction="#getRoomSize" />
        <Button fx:id="bestätigenButton" layoutX="116.0" layoutY="185.0" mnemonicParsing="false" onAction="#updateRoom" text="Update" />
        <Button fx:id="backToRoomOverviewButton" layoutX="76.0" layoutY="348.0" mnemonicParsing="false" onAction="#switchToStartScene" text="Zurück zur Room Overview" />
        <TextField fx:id="roomId" layoutX="154.0" layoutY="51.0" onAction="#getRoomId" />
        <Label layoutX="37.0" layoutY="55.0" text="Raum ID eingeben" />
      <Label layoutX="57.0" layoutY="223.0" text="Light ID" />
      <TextField fx:id="lightId" layoutX="102.0" layoutY="219.0" onAction="#getLightId" />
      <Button fx:id="addLightButton" layoutX="104.0" layoutY="304.0" mnemonicParsing="false" onAction="#addLight" text="Add Light" />
      <TextField fx:id="lightName" layoutX="102.0" layoutY="257.0" onAction="#getNameOfLight" />
      <Label layoutX="62.0" layoutY="261.0" text="Name" />
      <TextField fx:id="people" layoutX="362.0" layoutY="147.0" onAction="#getPeople" />
      <Label layoutX="311.0" layoutY="151.0" text="People" />
      <Button fx:id="addPeopleButton1" layoutX="362.0" layoutY="185.0" mnemonicParsing="false" onAction="#addPeople" text="Add People" />
    </children>
</AnchorPane>
