<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.jku.Controller">
   <children>
      <Label layoutX="113.0" layoutY="71.0" text="Licht erstellen">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="65.0" layoutY="198.0" text="Name" />
      <TextField fx:id="lightId" layoutX="112.0" layoutY="161.0" onAction="#getLightId" />
      <TextField fx:id="lightName" layoutX="112.0" layoutY="195.0" onAction="#getNameOfLight" />
      <Button fx:id="bestätigenButton" layoutX="151.0" layoutY="232.0" mnemonicParsing="false" onAction="#addLight" text="Bestätigen" />
      <Button fx:id="backToRoomOverviewButton" layoutX="90.0" layoutY="336.0" mnemonicParsing="false" onAction="#switchToStartScene" text="Zurück zur Room Overview" />
      <Label layoutX="57.0" layoutY="165.0" text="Light ID" />
      <Label fx:id="createdLight" layoutX="102.0" layoutY="294.0" text="." />
      <ComboBox fx:id="comboBox1" layoutX="112.0" layoutY="121.0" prefWidth="150.0" />
   </children>
</AnchorPane>
